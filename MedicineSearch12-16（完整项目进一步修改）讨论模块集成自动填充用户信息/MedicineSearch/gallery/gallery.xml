<?xml version="1.0" encoding="utf-8"?>
<gallery>
	<galleryName>3D Wall Gallery</galleryName>
	<settings>
		<imagesFolder>gallery/images/</imagesFolder>
		<thumbnailsFolder>gallery/images/</thumbnailsFolder>

		<visibleMatrix rows="3" />
		<zoom>1</zoom>
		<image scaleMode="fill" bgColor="0x000000" bgAlpha="50" thumbProportion="true"/>
		<imageCaption position="top" color="0xFFFFFF" bgColor="0x000000" bgAlpha="80" fontName="Arial" fontSize="11" visibleMode="always"/>
		<thumbnail width="200" height="150" distance="5" />
		<preloader
			stripesAlpha="45"
			barColor="0x000000"
			barAlpha="50"
			bgColor="0x000000"
			bgAlpha="10"
			innerShadow="10"
			dropShadow="30"
		/>
		<scrollSpeed>0.3</scrollSpeed>
		<showReflections>true</showReflections>
		<reflectionAlpha>80</reflectionAlpha>
		<useHighlight>true</useHighlight>
	</settings>
